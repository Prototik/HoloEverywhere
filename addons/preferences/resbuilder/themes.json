{
	"styles": {
		"blocks": {
			"PreferenceBase": {
				"checkBoxPreferenceStyle": "@style/Holo.PreferenceCheckBox", 
				"dialogPreferenceStyle": "@style/Holo.PreferenceDialog", 
                "editTextPreferenceStyle": "@style/Holo.PreferenceDialog.EditTextPreference",
                "numberPickerPreferenceStyle": "@style/Holo.PreferenceDialog.NumberPickerPreference", 
                "preferenceCategoryStyle": "@style/Holo.PreferenceCategory", 
                "preferenceFragmentStyle": "@style/Holo.PreferenceFragment", 
                "preferenceFrameLayoutStyle": "@style/Holo.PreferenceFrameLayout", 
                "preferenceInformationStyle": "@style/Holo.PreferenceInformation", 
                "preferenceLayoutChild": "@layout/preference_child_holo", 
                "preferencePanelStyle": "@style/Holo.PreferencePanel", 
                "preferenceScreenStyle": "@style/Holo.PreferenceScreen", 
