<?xml version="1.0" encoding="utf-8"?><resources>
    <!--Start attrs definition block-->
    <attr name="demoListRowViewStyle" format="reference"/>
    <attr name="disableMusicIcon" format="reference"/>
    <attr name="menuBackground" format="reference|color"/>
    <attr name="selectionHandler" format="color|reference"/>
    <attr name="selectionHandlerVisiblity" format="enum">
        <enum name="invisible" value="1"/>
        <enum name="visible" value="0"/>
    </attr>
    <attr name="titleLayout" format="reference"/>
    <!--Start styleable definition block-->
    <declare-styleable name="DemoListRowView">
        <attr name="android:background"/>
        <attr name="android:gravity"/>
        <attr name="android:text"/>
        <attr name="selectionHandler"/>
        <attr name="selectionHandlerVisiblity"/>
    </declare-styleable>
    <declare-styleable name="WidgetContainer">
        <attr name="android:layout"/>
        <attr name="android:text"/>
        <attr name="titleLayout"/>
    </declare-styleable>
    <!--End of any blocks-->
</resources>
