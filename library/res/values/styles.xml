        <item name="android:background">@drawable/btn_group_holo_light</item>
    </style>
    <style name="Holo.Spinner">
        <item name="android:background">@drawable/spinner_background_holo_dark</item>
        <item name="android:clickable">true</item>
        <item name="android:dropDownSelector">@drawable/list_selector_holo_dark</item>
        <item name="android:dropDownWidth">wrap_content</item>
        <item name="android:gravity">left|center_vertical</item>
        <item name="android:popupBackground">@drawable/menu_dropdown_panel_holo_dark</item>
        <item name="dropDownHorizontalOffset">0dip</item>
        <item name="dropDownVerticalOffset">0dip</item>
        <item name="spinnerMode">dropdown</item>
