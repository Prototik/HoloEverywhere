    <style name="Holo.Switch.Light">
        <item name="switchTextAppearance">@style/Holo.Switch.TextAppearance.Light</item>
        <item name="thumb">@drawable/switch_inner_holo_light</item>
        <item name="track">@drawable/switch_track_holo_light</item>
    </style>
    <style name="Holo.Switch.Light.Old">
        <item name="thumb">@drawable/switch_old_inner_holo_light</item>
    </style>
    <style name="Holo.Switch.Old">
        <item name="thumb">@drawable/switch_old_inner_holo_dark</item>
    </style>
    <style name="Holo.Switch.TextAppearance" parent="Holo.TextAppearance.Small">
        <item name="android:textSize">12sp</item>
    </style>
    <style name="Holo.Switch.TextAppearance.Light" parent="Holo.TextAppearance.Small.Light">
        <item name="android:textSize">12sp</item>
    </style>
    <style name="Holo.TextView">
        <item name="android:textAppearance">?android:attr/textAppearanceSmall</item>
    </style>
    <style name="Holo.TextView.ListSeparator">
        <item name="android:background">@drawable/list_section_divider_holo_dark</item>
        <item name="android:gravity">center_vertical</item>
        <item name="android:layout_height">wrap_content</item>
