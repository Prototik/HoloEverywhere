    <style name="Holo.TextView.ListSeparator.Light">
        <item name="android:background">@drawable/list_section_divider_holo_light</item>
    </style>
    <style name="Holo.TextView.SpinnerItem">
        <item name="android:gravity">left|center_vertical</item>
        <item name="android:paddingLeft">8dp</item>
        <item name="android:paddingRight">8dp</item>
        <item name="android:textAppearance">@style/Holo.TextAppearance.TextView.SpinnerItem</item>
        <item name="gravity">left|center_vertical</item>
