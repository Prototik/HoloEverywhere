    <style name="Holo.Spinner.Light">
        <item name="android:background">@drawable/spinner_background_holo_light</item>
        <item name="android:dropDownSelector">@drawable/list_selector_holo_light</item>
        <item name="android:popupBackground">@drawable/menu_dropdown_panel_holo_light</item>
    </style>
    <style name="Holo.Switch">
        <item name="switchMinWidth">96dp</item>
        <item name="switchPadding">16dp</item>
        <item name="switchTextAppearance">@style/Holo.Switch.TextAppearance</item>
        <item name="textOff">@string/capital_off</item>
        <item name="textOn">@string/capital_on</item>
        <item name="thumb">@drawable/switch_inner_holo_dark</item>
        <item name="thumbTextPadding">12dp</item>
        <item name="toggleWhenClick">true</item>
        <item name="track">@drawable/switch_track_holo_dark</item>
